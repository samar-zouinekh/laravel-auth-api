{
    "name": "samar-zouinekh/laravel-auth-api",
    "description": "Laravel Auth over api",
    "keywords": [
        "samar-zouinekh",
        "laravel-auth-api"
    ],
    "homepage": "https://github.com/samar-zouinekh",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Soufiene Slimi",
            "email": "soufiene.slimi@medianet.com.tn",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/passport": "^9.0|^10.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.3.3",
        "orchestra/testbench": "^5.3|^6.21"
    },
    "autoload": {
        "psr-4": {
            "SamarZouinekh\\LaravelAuthApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SamarZouinekh\\LaravelAuthApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SamarZouinekh\\LaravelAuthApi\\LaravelAuthApiServiceProvider"
            ],
            "aliases": {
                "LaravelAuthApi": "SamarZouinekh\\LaravelAuthApi\\LaravelAuthApiFacade"
            }
        }
    }
}
